<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.sourceforge.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pwa.velib.Station" table="VELIB_STATION">
        <id name="number">
        </id>
        <property name="name" column="label"/>
        <property name="address"/>
        <property name="position"/>
        <property name="status"/>
        <property name="bonus"/>
        <property name="bikeStands"/>
        <property name="availableBikes"/>
    </class>

</hibernate-mapping>
